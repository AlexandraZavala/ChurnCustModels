{
    "version": 2,
    "builds": [
      {
        "src": "web/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "web/main.py"
      }
    ],
    "installCommand": "pip install -r web/requirements.txt",
    "buildCommand": "streamlit run web/main.py"
  }
  